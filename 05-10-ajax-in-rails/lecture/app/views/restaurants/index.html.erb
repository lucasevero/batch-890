<div class="container my-5">
  <h1>My restaurants</h1>

  <ul>
    <% @restaurants.each do |restaurant| %>
      <li><%= link_to restaurant.name, restaurant_path(restaurant) %> - <%= link_to "Delete", restaurant_path(restaurant), method: :delete, remote: true, data: { confirm: "Are you sure?" } %></li>
    <% end %>
  </ul>
</div>
